<web-app>
  <display-name>Get your movie!</display-name>
  <description>
    A simple, servlet used as a client for a web service that issues a streaming link to a user depending on their favorite actor and movie.
  </description>
  <servlet>
    <servlet-name>Actor</servlet-name>
    <servlet-class>com.wartest.Client.WebService1Client</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Actor</servlet-name>
    <url-pattern>/actor</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>RecommendMovie</servlet-name>
    <servlet-class>com.wartest.Client.RecommendMovie</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RecommendMovie</servlet-name>
    <url-pattern>/RecommendMovie</url-pattern>
  </servlet-mapping>
</web-app>
